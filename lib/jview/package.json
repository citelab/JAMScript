{
  "name": "treeflow.js",
  "version": "1.0.0",
  "description": "A React+Mobx+Socket single way flow data visualization and analytics generator. ",
  "main": "ViewCompiler.js",
  "scripts": {
    "test": "node Compiler.js Config.json",
    "sample-1": "node Compiler.js doc/Samples/1.Basic-Formset/Config.json",
    "sample-2": "node Compiler.js doc/Samples/2.Basic-Table/Config.json",
    "sample-3": "node Compiler.js doc/Samples/3.Scatter-Over-Socket/Config.json; cp doc/Samples/3.Scatter-Over-Socket/Data-mocker.js ./app",
    "sample-4": "node Compiler.js doc/Samples/4.Multiple-Devices/Config.json; cp doc/Samples/4.Multiple-Devices/Data-mocker.js ./app",
    "sample-5": "node Compiler.js doc/Samples/5.Multiple-Panels/Config.json; cp doc/Samples/5.Multiple-Panels/Data-mocker.js ./app",
    "sample-6": "node Compiler.js doc/Samples/6.Multiple-Socket-IDs/Config.json; cp doc/Samples/6.Multiple-Socket-IDs/Data-mocker.js ./app",
    "sample-7": "node Compiler.js doc/Samples/7.Bar-Scroll/Config.json; cp doc/Samples/7.Bar-Scroll/Data-mocker.js ./app",
    "sample-8": "node Compiler.js doc/Samples/8.Max-Mem-Size/Config.json; cp doc/Samples/8.Max-Mem-Size/Data-mocker.js ./app",
    "sample-9": "node Compiler.js doc/Samples/9.All-Widgets/Config.json",
    "sample-10": "node Compiler.js doc/Samples/10.Button-Terminal/Config.json; cp doc/Samples/10.Button-Terminal/Data-mocker.js ./app",
    "sample-11": "node Compiler.js doc/Samples/11.Multistate-Button-Terminal/Config.json; cp doc/Samples/11.Multistate-Button-Terminal/Data-mocker.js ./app",
    "sample-12": "node Compiler.js doc/Samples/12.Multistate-Button-Interval/Config.json",
    "beautify": "find ./app -regex '.*\\.js' -exec js-beautify {} -r \\;"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gazcn007/treeflow.git"
